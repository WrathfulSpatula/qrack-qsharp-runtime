<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="Current">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <FileUpgradeFlags>40</FileUpgradeFlags>
    <UpgradeBackupLocation>C:\Users\jerse\Documents\GitHub\qrack-qsharp-runtime\Backup\</UpgradeBackupLocation>
    <OldToolsVersion>2.0</OldToolsVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.0</Version>
    <PackageProjectUrl>https://qrack.readthedocs.io/en/latest/</PackageProjectUrl>
    <PackageIcon>qrack_icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/vm6502q/qrack-qsharp-runtime</RepositoryUrl>
    <PackageTags>quantum-computing</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Quantum.CsharpGeneration" Version="0.12.20072031" />
    <PackageReference Include="Microsoft.Quantum.QSharp.Core" Version="0.12.20072031" />
    <PackageReference Include="Microsoft.Quantum.Runtime.Core" Version="0.12.20072031" />
    <PackageReference Include="Microsoft.Quantum.Simulators" Version="0.12.20072031" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\qrack_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <PropertyGroup Condition=" '$(OS)' == 'Windows_NT' ">
    <DefineConstants>_WINDOWS</DefineConstants>
  </PropertyGroup>
</Project>